<?xml version="1.0"?>
<!--
/**
 * Lmarcho RagSync Module - Dashboard Layout
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Lmarcho_RagSync::css/dashboard.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Lmarcho\RagSync\Block\Adminhtml\Dashboard"
                   name="ragsync.dashboard"
                   template="Lmarcho_RagSync::dashboard.phtml"/>
        </referenceContainer>
    </body>
</page>
